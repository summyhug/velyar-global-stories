<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Allow exposing videos saved under app's external files dir (Movies) -->
    <external-files-path name="external_movies" path="Movies/" />
    <!-- Also allow cache if we switch to cache-based temp files later -->
    <cache-path name="cache" path="." />
    <files-path name="files" path="." />
</paths>